{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.0.0",
    "babel-plugin-relay": "^8.0.0",
    "cuid": "^2.1.8",
    "nullthrows": "^1.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-relay": "^8.0.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.1",
    "react-window": "^1.8.6",
    "relay-query-lookup-renderer": "^6.0.0",
    "relay-runtime": "^8.0.0",
    "styled-components": "^5.0.0",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "relay": "relay-compiler --src ./src --schema ../schema.graphql --language typescript",
    "chk": "yarn fmt:check && yarn lint && yarn tsc",
    "fmt": "./node_modules/.bin/prettier --write \"src/**/*.ts\"",
    "fmt:check": "./node_modules/.bin/prettier --check \"src/**/*.ts\"",
    "lint": "./node_modules/.bin/eslint --config .eslintrc \"src/**/*.*\"",
    "lint:fix": "./node_modules/.bin/eslint --fix --config .eslintrc \"src/**/*.*\"",
    "tsc": "./node_modules/.bin/tsc --noEmit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-relay": "^7.0.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/relay-runtime": "^8.0.6",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "graphql": "^14.6.0",
    "husky": "^4.3.0",
    "prettier": "^1.19.1",
    "relay-compiler": "^8.0.0",
    "relay-compiler-language-typescript": "^10.1.4",
    "relay-config": "^8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn chk"
    }
  }
}
