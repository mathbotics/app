{
  "type": "module",
  "scripts": {
    "chk": "yarn fmt:check && yarn lint && yarn tsc",
    "dev": "tsnd --no-notify --transpileOnly --respawn ./src/app/index.ts",
    "fix": "yarn fmt && yarn lint:fix",
    "fmt": "./node_modules/.bin/prettier --write 'src/**/*.ts'",
    "fmt:check": "./node_modules/.bin/prettier --check 'src/**/*.ts'",
    "gen": "yarn gen:prisma && yarn gen:nexus",
    "gen:nexus": "tsnd --no-notify --transpileOnly --project ./scripts/tsconfig.json ./scripts/gen/nexus.ts",
    "gen:prisma": "./node_modules/.bin/prisma2 generate",
    "lint": "./node_modules/.bin/eslint --config .eslintrc 'src/**/*.*'",
    "lint:fix": "./node_modules/.bin/eslint --fix --config .eslintrc 'src/**/*.*'",
    "migrate:save": "./node_modules/.bin/prisma2 migrate save --experimental",
    "migrate:up": "./node_modules/.bin/prisma2 migrate up --experimental",
    "migrate:down": "./node_modules/.bin/prisma2 migrate down --experimental",
    "postinstall": "yarn gen",
    "start": "ts-node ./src/app/index.ts",
    "seed": "DATABASE_URL='postgres://postgres:password@localhost:5432/postgres' ./node_modules/.bin/tsnd --no-notify --transpileOnly",
    "test": "./node_modules/.bin/jest",
    "tsc": "./node_modules/.bin/tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-preview021",
    "apollo-server-express": "^2.10.0",
    "bcrypt": "^3.0.8",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.9",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^0.12.0-rc.9",
    "nexus-prisma": "^0.9.1",
    "nullthrows": "^1.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/jest": "^25.1.1",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.19.0",
    "apollo-server-testing": "^2.10.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "pg": "^7.18.1",
    "prettier": "^1.19.1",
    "prisma2": "^2.0.0-preview021",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.1-rc"
  },
  "husky": {
    "hooks": {}
  },
  "license": "MIT"
}
