{
  "scripts": {
    "chk": "yarn fmt:check && yarn lint && yarn tsc",
    "dev": "tsnd --no-notify --transpileOnly --respawn ./src/app/index.ts",
    "devwin": "env.bat && tsnd --no-notify --transpileOnly --respawn ./src/app/index.ts",
    "devmac": "bash ./env.sh && tsnd --no-notify --transpileOnly --respawn ./src/app/index.ts",
    "docker:clean": "docker rm $(docker ps -a -q) && docker rmi $(docker images -a -q)",
    "docker:clean:images": "docker rmi $(docker images -a -q)",
    "docker:clean:containers": "docker rm $(docker ps -a -q)",
    "docker:down": "cd ../docker && docker-compose down",
    "docker:up": "cd ../docker && docker-compose up",
    "fix": "yarn fmt && yarn lint:fix",
    "fmt": "./node_modules/.bin/prettier --write \"src/**/*.ts\"",
    "fmt:check": "./node_modules/.bin/prettier --check \"src/**/*.ts\"",
    "gen": "yarn gen:prisma",
    "gen:prisma": "prisma generate",
    "lint": "./node_modules/.bin/eslint --config .eslintrc \"src/**/*.*\"",
    "lint:fix": "./node_modules/.bin/eslint --fix --config .eslintrc \"src/**/*.*\"",
    "migrate:save": "prisma migrate dev --preview-feature",
    "migrate:up": "prisma migrate deploy --preview-feature",
    "migrate:down": "",
    "start": "ts-node ./src/app/index.ts",
    "seed": "DATABASE_URL='postgres://postgres:password@localhost:5432/postgres' ./node_modules/.bin/tsnd --no-notify --transpileOnly",
    "seedwin": "SET DATABASE_URL=postgres://postgres:password@localhost:5432/postgres && %cd%\\node_modules\\.bin\\tsnd --no-notify --transpileOnly",
    "adminmac": "yarn seed src/seeders/AdminSeeder.ts",
    "adminwin": "yarn seedwin src/seeders/AdminSeeder.ts",
    "test": "./node_modules/.bin/jest",
    "tsc": "./node_modules/.bin/tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^2.16.0",
    "@types/mjml": "^4.0.3",
    "apollo-server-express": "^2.19.2",
    "bcrypt": "^3.0.8",
    "cookie-parser": "^1.4.4",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.0.3",
    "graphql-shield": "^7.5.0",
    "handlebars": "^4.7.3",
    "jsonwebtoken": "^8.5.1",
    "mjml": "^4.5.1",
    "nodemailer": "^6.4.4",
    "nullthrows": "^1.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/jest": "^25.1.1",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.19.0",
    "apollo-server-testing": "^2.19.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "pg": "^7.18.1",
    "prettier": "^1.19.1",
    "prisma": "2.16.0",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.9.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.1-rc"
  },
  "husky": {
    "hooks": {
      "pre-push": ""
    }
  },
  "license": "MIT"
}
